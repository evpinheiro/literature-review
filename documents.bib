doi = {10.1007/s00291-005-0207-9},
file = {:home/eder/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Gintner, Kliewer, Suhl - 2005 - Solving large multiple-depot multiple-vehicle-type bus scheduling problems in practice(2).pdf:pdf},
