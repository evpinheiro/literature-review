@article{Bertuol2010,
abstract = {A utilidade dos produtos n{\~{a}}o {\'{e}} o {\'{u}}nico fator determinante nas decis{\~{o}}es de compra dos consumidores, na medida em que os significados simb{\'{o}}licos carregados pelos bens tamb{\'{e}}m exercem papel fundamental nesse processo. Estes significados podem ser tanto idiossincr{\'{a}}ticos, quanto comumente partilhados com outros consumidores. Al{\'{e}}m disso, podem ser utilizados pelos indiv{\'{i}}duos para sustentar a sua imagem ou criar uma nova representa{\c{c}}{\~{a}}o de si mesmos. O presente estudo buscou investigar o processo de constru{\c{c}}{\~{a}}o de significados agregados ao consumo de vinhos por um grupo de mulheres que dividem h{\'{a}}bitos, compartilham gostos e opini{\~{o}}es por meio da pr{\'{a}}tica de rituais. Esses eventos deram origem {\`{a}} forma{\c{c}}{\~{a}}o de uma confraria do vinho na Serra Ga{\'{u}}cha. Neste contexto, destaca-se a presen{\c{c}}a de dois fen{\^{o}}menos: o primeiro referente ao aumento do consumo de vinhos pelos brasileiros, tornando o produto mais popular e acess{\'{i}}vel para um n{\'{u}}mero maior de pessoas; o segundo fen{\^{o}}meno diz respeito {\`{a}} forma{\c{c}}{\~{a}}o e prolifera{\c{c}}{\~{a}}o das chamadas “confrarias do vinho” – grupos de indiv{\'{i}}duos que se re{\'{u}}nem basicamente para degustar e estudar as caracter{\'{i}}sticas e propriedades dos vinhos. A fim de atingir o prop{\'{o}}sito do estudo, uma pesquisa qualitativa e explorat{\'{o}}ria foi conduzida. Os materiais disponibilizados pela confraria e os depoimentos de tr{\^{e}}s de suas integrantes serviram de base para a an{\'{a}}lise dos resultados do estudo e foram interpretados atrav{\'{e}}s da an{\'{a}}lise de discurso. Al{\'{e}}m de incentivar o conv{\'{i}}vio social, a aprecia{\c{c}}{\~{a}}o ao vinho promove a troca de experi{\^{e}}ncias e o aprendizado sobre o universo hist{\'{o}}rico-cultural da bebida. Consequentemente, a partir da organiza{\c{c}}{\~{a}}o da confraria, novos grupos de consumidoras e formadoras de opini{\~{o}}es se desenvolvem. Dentre os principais resultados, constatou-se que a confraria exerce papel fundamental na mudan{\c{c}}a do estilo de vida das participantes. Por meio dela, as mulheres reafirmam o seu papel na sociedade, conquistando o seu espa{\c{c}}o em uma {\'{a}}rea tradicionalmente dominada pelos homens. Portanto, al{\'{e}}m de ser uma bebida familiar para elas, o vinho se torna um s{\'{i}}mbolo de intelectualidade, poder, desafio, prazer, socializa{\c{c}}{\~{a}}o e hobby.},
author = {Bertuol, M.V. and Brasil, V. S.},
doi = {10.5902/19834659},
file = {:home/eder/Downloads/Resolucao{\_}integrada{\_}de{\_}problemas{\_}do{\_}planejamento{\_}d.pdf:pdf},
issn = {1983-4659},
journal = {IV Encontro de Marketing da ANPAD},
keywords = {Comportamento do Consumidor,Consumo Feminino,Mulheres,Significados de Consumo,Vinho},
mendeley-groups = {Problems/PublicTransitPlanning/VS},
pages = {1--17},
title = {{Resolu{\c{c}}{\~{a}}o integrada de problemas do planejamento do transporte p{\'{u}}blico: foco na tabela de hor{\'{a}}rios e no escalonamento de ve{\'{i}}culos com frota hegerog{\^{e}}nea}},
year = {2010}
}
@article{Ribeiro1994,
abstract = {We give a new formulation to the multiple-depot vehicle scheduling problem as a set partitioning problem with side constraints, whose continuous relaxation is amenable to be solved by column generation. We show that the continuous relaxation of the set partitioning formulation provides a much tighter lower bound than the additive bound procedure previously applied to this problem. We also establish that the additive bound technique cannot provide tighter bounds than those obtained by Lagrangian decomposition, in the framework in which it has been used so far. Computational results that illustrate the robustness of the combined set partitioning-column generation approach are reported for problems four to five times larger than the largest problems that have been exactly solved in the literature. Finally, we show that the gap associated with the additive bound based on the assignment and shortest path relaxations can be arbitrarily bad in the general case, and as bad as 50{\%} in the symmetric case.},
annote = {Take a look on Theorem 1.},
author = {Ribeiro, Celso C and Soumis, Fran{\c{c}}ois},
file = {:home/eder/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Ribeiro, Soumis - 1994 - A Column Generation Approach to the Multiple-Depot Vehicle Scheduling Problem.pdf:pdf},
journal = {Operations Research},
mendeley-groups = {Problems/PublicTransitPlanning/VS},
title = {{A Column Generation Approach to the Multiple-Depot Vehicle Scheduling Problem}},
volume = {42},
year = {1994}
}
